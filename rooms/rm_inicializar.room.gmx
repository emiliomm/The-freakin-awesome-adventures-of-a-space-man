  <code>///Variables globales del juego que se inician al principio del juego

global.version = 1.2; //Version actual del juego

global.sound = 1; //Si suena sonido o no 1 = si, 0 = no
global.fullscreen = 1; //1 = Pantalla completa, 0 = ventana
global.pantalla_inicial = 0; //Has visto la pantalla inicial

//Datos del jugador
global.ranura = 1; //Ranura actual del jugador (1-3)
global.fase_actual = 0; //Fase actual del jugador
global.puntuacion = 0; //Puntuación total del jugador

global.font1 = font_add_sprite_ext(spr_font, " !#$%&amp;'(),-./0123456789:;&lt;=&gt;?@ABCDEFGHIJKLMNOPQRSTUVWXYZ[\]^_abcdefghijklmnopqrstuvwxyz", false, 0.5);
global.font2 = font_add_sprite(spr_font_text, ord("!"), 1,1);

//Sistema de checkpoint
global.checkpoint = noone; //Checkpoint activo
global.checkpointR = 0; //Número de habitación
global.checkpointx = 0; //Coordenadas
global.checkpointy = 0;

global.roomcamefrom = noone; //Habitacion donde se guarda donde el heroe ha muerto

//Sistema de puertas
global.door = false;
global.doorx = 0;
global.doory = 0;

randomize(); //Genera aleatoriedad real

//Mando legacy
if (joystick_exists(0) &amp;&amp; joystick_buttons(0) &gt;= 10) {
    global.joystick1_supported = true;
    global.joy_w = false;
    global.joy_space = false;
    global.joy_right = false;
    global.joy_left = false;
    global.joy_up = false;
    global.joy_down = false;
    global.joy_escape = false;
    global.joy_m = false;
    global.joy_h = false;
    window_set_cursor(cr_none); //Ocultamos el cursor del raton
}
else 
    global.joystick1_supported = false;
    
scr_cargar_preferencias();

room_goto_next(); //Va a la siguiente habitación automaticamente sin renderizar nada</code>
